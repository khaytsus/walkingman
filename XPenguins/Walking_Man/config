# This is the config file for the xpenguins theme "Walking Man".
# Walter Francis <wally@theblackmoor.net>
# 29 July 2002

delay 60 
number 20

define default
	width 20
	height 20
	frames 10
	speed 0
	directions 1 

define floater	pixmap	floater.xpm height 32 speed 3 frames 2
define walker	pixmap	walking.xpm speed 2
define faller	pixmap	falling.xpm speed 3 frames 4 acceleration 1
define tumbler	pixmap	tumble.xpm speed 2 frames 4 acceleration 3 terminal_velocity 8
define climber	pixmap	climber.xpm speed 2 directions 2 frames 3
define action0	pixmap	looking.xpm frames 6 loop -15
define action1	pixmap	dancing.xpm loop -10
define action2	pixmap	walkinglight.xpm speed 2 width 40 directions 2 frames 2 loop -10
define action3	pixmap	sitting.xpm frames 4 loop -10
define action4	pixmap	sleep.xpm directions 2 frames 1 loop -80
define action5	pixmap	juggle.xpm height 32 directions 2 frames 10 loop 5
define splatted pixmap	splat.xpm frames 5
define zapped	pixmap	explosion.xpm height 32
define explosion	pixmap	explosion.xpm height 32
define angel	pixmap	angel.xpm frames 6 height 32 speed 3
define exit	pixmap	splat.xpm frames 5
